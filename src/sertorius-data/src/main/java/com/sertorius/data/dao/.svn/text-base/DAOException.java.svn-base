package com.arretium.base.dao;

/***
 * Generic DAO Exception<br>
 * <br>
 *
 * @author caesar
 * @version 1.0
 */
public class DAOException extends Exception {

    private static final long serialVersionUID = -3832071547718834279L;


    public DAOException() {
        super();
    }

    public DAOException(final String message) {
        super(message);
    }

    public DAOException(final Throwable throwable) {
        super(throwable);
    }

    public DAOException(final String message, final Throwable throwable) {
        super(message, throwable);
    }
}
